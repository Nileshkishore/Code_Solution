#Find Lucky Integer in an Array

class Solution:
    def fib(self, n: int) -> int:
        if n==0:
            return 0
        elif  n==1:
            return 1
        else:
            n1=0
            n2=1
            c=2
            for i in range(2,n+1):
                res=n1+n2
                n1=n2
                n2=res
            return res
